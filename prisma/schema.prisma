// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Question {
  id          Int     @id @default(autoincrement())
  question    String
  subText     String?
  description String?
  required    Boolean @default(false)
  type        String?
  placeholder String?
}
