generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Question {
  id          Int     @id @default(autoincrement())
  question    String
  subText     String?
  description String?
  required    Boolean @default(false)
  type        String?
  placeholder String?
}

model Theme {
  id        Int     @id @default(autoincrement())
  primary   String?
  secondary String?
}
